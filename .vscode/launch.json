    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\www",
            "env": {
                "POLICY_SERVICE_URL": "http://localhost:8210/getAllPolicies",
                "ELIGIBILITY_SERVICE_URL": "http://localhost:8210/getBenefits",
                "LOG_LEVEL": "info",
                "ENROLLMENT_SERVICE_URL": "http://localhost:8210/enrollment",
                "POLICY_DETAILS_URL": "http://localhost:8210/getPolicyDetails"
            }
        }
    ]
}